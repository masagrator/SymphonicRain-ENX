				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x750",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x752",
				"TYPE": "Text",
				"STRING": [
					"I know I can be."
				]
			},
