			{
				"LABEL": "0x184",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x186",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x188",
				"TYPE": "Text",
				"STRING": [
					"I just couldn't."
				]
			},
			{
				"LABEL": "0x190",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x192",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x194",
				"TYPE": "Text",
				"STRING": [
					"Torta and I were too close."
