			},
			{
				"LABEL": "0x1ec",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x1ee",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x1f0",
				"TYPE": "Text",
				"STRING": [
