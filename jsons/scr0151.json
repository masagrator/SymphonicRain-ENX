			{
				"LABEL": "0x222",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x224",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x226",
				"TYPE": "Text",
				"STRING": [
