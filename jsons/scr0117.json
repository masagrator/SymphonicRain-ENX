			{
				"LABEL": "0x1e4",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x1e6",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x1e8",
				"TYPE": "Text",
				"STRING": [
