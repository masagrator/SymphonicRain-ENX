			{
				"LABEL": "0x1ce",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x1d0",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x1d2",
				"TYPE": "Text",
