			{
				"LABEL": "0x134",
				"TYPE": "NewLine"
			},
			{
				"LABEL": "0x136",
				"TYPE": "Text",
				"STRING": [
					"Phorni's song was almost ready."
				]
			},
			{
				"LABEL": "0x13e",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x140",
