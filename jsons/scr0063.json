			{
				"LABEL": "0x110",
				"TYPE": "Text",
				"STRING": [
					"Having abandoned all appointments and practice plans,",
					"I was in a practice room."
				]
			},
			{
				"LABEL": "0x118",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x11a",
				"TYPE": "NewLine"
			},
			{
				"LABEL": "0x11c",
				"TYPE": "NewLine"
			},
			{
				"LABEL": "0x11e",
				"TYPE": "Text",
				"STRING": [
					"I'd grown fond of the score I was copying, which was as",
					"important to me as Phorni was."
				]
			},
			{
				"LABEL": "0x130",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x132",
