			},
			{
				"LABEL": "0x738",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x73a",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x73c",
				"TYPE": "Text",
				"STRING": [
