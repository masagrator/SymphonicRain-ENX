			},
			{
				"LABEL": "0x6aa",
				"TYPE": "Text",
				"STRING": [
					"[Schoolgirl] “Y-Yes.”"
				]
			},
			{
				"LABEL": "0x6bc",
				"TYPE": "KeyWait"
			},
			{
				"LABEL": "0x6be",
				"TYPE": "NewPage"
			},
			{
				"LABEL": "0x6c0",
				"TYPE": "Text",
				"STRING": [
